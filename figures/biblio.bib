keywords = {Buildings,Distributed approach,Internet,Internet of Things,Logic gates,Measurement,Node abstraction,Node selection,Routing,Search problems,Semantic routing,Semantics,Web services,directed graphs,geographical locations,homogenous service descriptions,information aggregation,pattern clustering,recursive clustering,request matching,routing tables,scalability issue,scalable IoT service search,search path length,semantic Web,semantic Web technologies,semantic gateways,semantic service descriptions,service query,similarity thresholds},

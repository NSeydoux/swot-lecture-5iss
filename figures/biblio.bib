journal = {IEEE Access},
keywords = {Computer architecture,Context awareness,Internet of Things,Interoperability,IoT-A architecture reference model,IoT-ARM,LMU-Compliant,Pervasive computing,RDF,Resource description framework,SIB,SPARQL,Semantics,Sensors,knowledge representation,local smart spaces,open systems,pervasive computing,physical object identifier,resolution infrastructure,semantic Web,semantic Web knowledge representation technologies,semantic information broker,semantic interoperability,semantic level interoperability architecture,system architecture,virtual representations},
